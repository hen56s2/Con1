local function runScript(url)
    loadstring(game:HttpGet(url))()
end

local co1 = coroutine.create(runScript)
local co2 = coroutine.create(runScript)

coroutine.resume(co1, "https://raw.githubusercontent.com/hen56s2/Con3/refs/heads/main/3")
coroutine.resume(co2, "https://raw.githubusercontent.com/hen56s2/Con2/refs/heads/main/2")
